
<% for(var i in rows){ var row = rows[i] %>
<tr data-rare="<%= row['rare'] ? row['rare'] : "-" %>" data-type="<%= row['job'] ? row['job'] : "-" %>" data-scene="<%= row['scene_type'] ? 'skill' : 'scene' %>" data-attr="<%= row['skill_attr'] ? row['skill_attr'] : "-" %><%= row['skill_ex1_attr'] ? row['skill_ex1_attr'] : "-" %><%= row['skill_ex2_attr'] ? row['skill_ex2_attr'] : "-" %>" >
    <td style="width:58px;"><%= parseInt(i)+1 %></td>
    <td><img src="<%= row['card_icon'] ? row['card_icon'] : "-" %>" /></td>
    <td>★<%= row['rare'] ? row['rare'] : "-" %></td>
    <td>
        <a target="_blank" href="<%= row['url'] ? row['url'] : "-" %>" data-id="<%= i %>">
            <%- row['title'] ? row['title'] : "-" %>
        </a>
    </td>
    <td><%- row['job'] ? row['job'] : "-" %></td>
    <td data-scene="<%= row['scene_type'] ? row['scene_type'] : "-" %>"><%= row['scene_type'] ? row['scene_type'] : "-" %></td>
