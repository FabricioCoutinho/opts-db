    <td class="total_cp" data-order="<%= row['total_cp'] ? row['total_cp'] : "-" %>"><strong><%= row['total_cp'] ? row['total_cp'] : "-" %> <span></span></strong></td>
    <td class="gp1_max max_hp" data-val="<%= row['max_hp'] ? row['max_hp'] : "-" %>"><%= row['max_hp'] ? row['max_hp'] : "-" %> <span></span></td>
    <td class="gp1_max max_sp" data-val="<%= row['max_sp'] ? row['max_sp'] : "-" %>"><%= row['max_sp'] ? row['max_sp'] : "-" %> <span></span></td>
